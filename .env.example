
# Environment variables for Sentinel-AV
VT_KEY=your_virustotal_api_key_here

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USER=sentinel
DB_PASSWORD=sentinel
DB_NAME=sentinel_av
DB_SSLMODE=disable
DB_TIMEZONE=UTC

# HTTP Server Configuration
HTTP_PORT=3000

# gRPC Server Configuration
GRPC_PORT=50051
# For TLS (optional, uncomment to enable)
# GRPC_CERT_FILE=/path/to/cert.pem
# GRPC_KEY_FILE=/path/to/key.pem

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Logging Configuration
LOG_LEVEL=info

# Application Configuration
SERVER_MODE=production

